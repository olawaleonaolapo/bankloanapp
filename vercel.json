{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/bankloanfabend.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/bankloanfabend.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/bankloanfabend.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/public/404.html"
    }
  ]
}